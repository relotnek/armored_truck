<%= form_for(@safe, :url => safe_upload_path, :method => 'post') do |f| %>
	<%= f.label "Private Key:" %><br>
	<%= f.file_field :priv_key %>
	<%= f.label :rawfile %>
	<%= f.file_field :rawfile %><br>
	<%= label_tag "Recipient Email:" %><br>
	<%= text_field_tag :recipient_id %><br>
<%= f.button :submit, :class => "btn btn-xs btn-success" %>
<% end %>

